{
	"env": {
		"browser": true,
		"node": true,
		"es2021": true
	},
	"plugins": ["import"],
	"extends": ["eslint:recommended", "plugin:import/recommended"],
	"overrides": [],
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"rules": {
		"semi": "off",
		"arrow-parens": "off",
		"comma-dangle": "off",
		"no-console": "off",
		"require-jsdoc": "off",
		"no-tabs": "off",
		"no-trailing-spaces": "off",
		"indent": "off",
		"no-debugger": "off",
		"padded-blocks": "off",
		"prefer-default-export": "off",
		"no-prototype-builtins": "off",
		"no-mixed-spaces-and-tabs": "off",
		"no-unused-vars": "warn",
		"no-undef": "error",
		"no-unused-expressions": "warn",
		"react/react-in-jsx-scope": "off",
		"import/no-unresolved": "error"
	},
	"settings": {
		"import/resolver": {
			"alias": {
				"map": [["@", "./src"]]
			},
			"node": {
				"extensions": [".js", ".jsx"]
			}
		}
	}
}
